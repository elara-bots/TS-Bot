{
  "name": "typescriptbot",
  "version": "1.0.0",
  "description": "Template for a TypeScript bot ",
  "main": "./dist/src/index.js",
  "scripts": {
    "start": "npm run pf && npm run build && npm run deploy && node ./dist/index.js",
    "build": "tsc",
    "deploy": "ts-node ./src/scripts/deploy.ts",
    "lint": "eslint .",
    "pf": "prettier --write src/**/*.ts",
    "pc": "prettier --check src/**/*.ts"
  },
  "keywords": [
    "typescript",
    "discord.js",
    "discord",
    "ts"
  ],
  "author": "SUPERCHIEFYT (Elara-Discord-Bots, Elara-Services)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.3.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@elara-services/botbuilder": "^1.0.5",
    "@elara-services/fetch": "^2.0.2",
    "@elara-services/packages": "^6.0.8",
    "@elara-services/utils": "^1.2.38",
    "discord.js": "^14.13.0"
  }
}
