{
  "name": "typescriptbot",
  "version": "1.0.0",
  "description": "Template for a TypeScript bot ",
  "main": "./dist/src/index.js",
  "scripts": {
    "start": "npm run prettier:format && npm run build && npm run deploy && node ./dist/index.js",
    "build": "tsc",
    "deploy": "ts-node ./src/scripts/deploy.ts",
    "lint": "eslint .",
    "prettier:format": "prettier --write src/**/*.ts",
    "prettier:check": "prettier --check src/**/*.ts"
  },
  "keywords": [
    "typescript",
    "discord.js",
    "discord",
    "ts"
  ],
  "author": "SUPERCHIEFYT (Elara-Discord-Bots, Elara-Services)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@elara-services/fetch": "^1.0.1",
    "@elara-services/packages": "^5.3.0",
    "@elara-services/utils": "^1.0.4",
    "discord.js": "^14.11.0"
  }
}
